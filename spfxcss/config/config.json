{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "spfxcss-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/spfxcss/SpfxcssApplicationCustomizer.js",
          "manifest": "./src/extensions/spfxcss/SpfxcssApplicationCustomizer.manifest.json"
        }
      ]
    },
    "addcustomfiles-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/addcustomfiles/AddcustomfilesApplicationCustomizer.js",
          "manifest": "./src/extensions/addcustomfiles/AddcustomfilesApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    
  },
  "localizedResources": {
    "SpfxcssApplicationCustomizerStrings": "lib/extensions/spfxcss/loc/{locale}.js",
    "AddcustomfilesApplicationCustomizerStrings": "lib/extensions/addcustomfiles/loc/{locale}.js"
  }
}
